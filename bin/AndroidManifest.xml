        <activity android:name="com.archy_assistant_WebBrowser.WebBrowser" >
        </activity>
        <activity
            android:name="com.archy_assistant_gallerydemo.NewFolder"
            android:label="  新建考古图片文件夹" >
        </activity>
        <activity
            android:name="com.archy_assistant.Activity07"
            android:label="  通信平台" >
