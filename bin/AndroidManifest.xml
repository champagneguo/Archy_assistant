        <activity
            android:name="com.archy_assistant_question.ChartActivity"
            android:label="  统计图表" >
        </activity>
        <activity
            android:name="com.archy_assistant.Activity03"
            android:configChanges="orientation|keyboardHidden"
            android:label="  搜索导航"
            android:screenOrientation="sensor" >
        </activity>
        <activity
            android:name="com.archy_assistant_map.Activity03_add"
            android:label="  考古点添加" >
